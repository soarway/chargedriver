&tlmm {
	usb1_vbus_boost {
		usb1_vbus_boost_default: usb1_vbus_boost_default {
				mux {
					pins = "gpio26";
					function = "gpio";
				};

				config {
					pins = "gpio26";
					output-high;
				};
		};
	};
	key_user {
		key_user_default: key_user_default {
			mux {
				pins = "gpio117";
				function = "gpio";
			};

			config {
				pins = "gpio117";
				bias-pull-up;
				input-enable;
			};
		};
	};

	ts_mux_goodix {
		ts_int_default: ts_int_default {
			mux {
				pins = "gpio1";
				function = "gpio";
			};

			config {
				pins = "gpio1";
				output-disable;
			};
		};

		ts_int_output_low: ts_int_output_low {
			mux {
				pins = "gpio1";
				function = "gpio";
			};

			config {
				pins = "gpio1";
				drive-strength = <16>;
				output-enable;
				output-low;
			};
		};

		ts_int_output_high: ts_int_output_high {
			mux {
				pins = "gpio1";
				function = "gpio";
			};

			config {
				pins = "gpio1";
				drive-strength = <16>;
				output-enable;
				output-high;
			};
		};

		ts_int_input: ts_int_input {
			mux {
				pins = "gpio1";
				function = "gpio";
			};

			config {
				pins = "gpio1";
				input-enable;
				bias-disable;
			};
		};
	};

	cam_vcm_active_rear: cam_vcm_active_rear {
		/* STANDBY REAR */
		mux {
			pins = "gpio97";
			function = "gpio";
		};

		config {
			pins = "gpio97";
			bias-disable; /* No PULL */
			drive-strength = <2>; /* 2 MA */
		};
	};
	cam_vcm_suspend_rear: cam_vcm_suspend_rear {
		/* STANDBY REAR */
		mux {
			pins = "gpio97";
			function = "gpio";
		};

		config {
			pins = "gpio97";
			bias-pull-down; /* PULL DOWN */
			drive-strength = <2>; /* 2 MA */
			output-low;
		};
	};

	cam_vcm_active_rear_aux: cam_vcm_active_rear_aux {
		/* STANDBY REAR */
		mux {
			pins = "gpio98";
			function = "gpio";
		};

		config {
			pins = "gpio98";
			bias-disable; /* No PULL */
			drive-strength = <2>; /* 2 MA */
		};
	};
	cam_vcm_suspend_rear_aux: cam_vcm_suspend_rear_aux {
		/* STANDBY REAR */
		mux {
			pins = "gpio98";
			function = "gpio";
		};

		config {
			pins = "gpio98";
			bias-pull-down; /* PULL DOWN */
			drive-strength = <2>; /* 2 MA */
			output-low;
		};
	};

	cam_vcm_active_front: cam_vcm_active_front {
		/* STANDBY FRONT */
		mux {
			pins = "gpio99";
			function = "gpio";
		};

		config {
			pins = "gpio99";
			bias-disable; /* No PULL */
			drive-strength = <2>; /* 2 MA */
		};
	};
	cam_vcm_suspend_front: cam_vcm_suspend_front {
		/* STANDBY FRONT */
		mux {
			pins = "gpio99";
			function = "gpio";
		};

		config {
			pins = "gpio99";
			bias-pull-down; /* PULL DOWN */
			drive-strength = <2>; /* 2 MA */
			output-low;
		};
	};

	camera_dvdd_en_default: camera_dvdd_en_default {
		mux {
			pins = "gpio159";
			function = "gpio";
		};

		config {
			pins = "gpio159";
			bias-disable; /* No PULL */
			drive-strength = <2>; /* 2 MA */
		};
	};

	spkr_1_sd_n {
		spkr_1_sd_n_sleep: spkr_1_sd_n_sleep {
			mux {
				pins = "gpio127";
				function = "gpio";
			};

			config {
				pins = "gpio127";
				drive-strength = <2>;   /* 2 mA */
				bias-pull-down;
				input-enable;
			};
		};

		spkr_1_sd_n_active: spkr_1_sd_n_active {
			mux {
				pins = "gpio127";
				function = "gpio";
 			};

			config {
				pins = "gpio127";
				drive-strength = <16>;   /* 16 mA */
				bias-disable;
				output-high;
			};
		};
	};

	spkr_2_sd_n {
		spkr_2_sd_n_sleep: spkr_2_sd_n_sleep {
			mux {
				pins = "gpio128";
				function = "gpio";
			};

			config {
				pins = "gpio128";
				drive-strength = <2>;   /* 2 mA */
				bias-pull-down;
				input-enable;
			};
		};

		spkr_2_sd_n_active: spkr_2_sd_n_active {
 			mux {
				pins = "gpio128";
				function = "gpio";
			};

			config {
				pins = "gpio128";
				drive-strength = <16>;   /* 16 mA */
				bias-disable;
				output-high;
			};
		};
	};
};

&vendor {
	openq_batterydata: qcom,battery-data {
		qcom,batt-id-range-pct = <15>;
		#include "fg-gen4-batterydata-powerizer-3350mah.dtsi"
	};
	extcon_usb1: extcon_usb1 {
		compatible = "linux,extcon-usb-gpio";
		id-gpio = <&tlmm 91 GPIO_ACTIVE_HIGH>;
		vbus-out-gpio = <&tlmm 26 GPIO_ACTIVE_HIGH>;

		pinctrl-names = "default";
		pinctrl-0 = <&usb2_id_det_default
				&usb1_vbus_boost_default>;
	};
};

&usb1 {
	extcon = <&extcon_usb1>;
};

&pm8150_l16 {
	qcom,init-voltage = <3304000>;
};

&soc {
	gpio_keys {
		compatible = "gpio-keys";
		label = "gpio-keys";

		pinctrl-names = "default";
		pinctrl-0 = <&key_vol_up_default
			&key_user_default>;

		vol_up {
			label = "volume_up";
			gpios = <&pm8150_gpios 6 GPIO_ACTIVE_LOW>;
			linux,input-type = <1>;
			linux,code = <KEY_VOLUMEUP>;
			gpio-key,wakeup;
			debounce-interval = <15>;
			linux,can-disable;
		};

		user {
			label = "user";
			gpios = <&tlmm 117 GPIO_ACTIVE_LOW>;
			linux,input-type = <1>;
			linux,code = <KEY_HOME>;
			gpio-key,wakeup;
			debounce-interval = <15>;
		};
	};

	leds {
		compatible = "gpio-leds";
		boot_status {
			gpios = <&tlmm 15 0>;
			linux,default-trigger = "heartbeat";
		};
	};
};

&soc {
	dsi_openq_panel_pwr_supply: dsi_openq_panel_pwr_supply {
		#address-cells = <1>;
		#size-cells = <0>;

		qcom,panel-supply-entry@0 {
			reg = <0>;
			qcom,supply-name = "vddio";
			qcom,supply-min-voltage = <1800000>;
			qcom,supply-max-voltage = <1800000>;
			qcom,supply-enable-load = <62000>;
			qcom,supply-disable-load = <80>;
			qcom,supply-post-on-sleep = <20>;
		};

		qcom,panel-supply-entry@1 {
			reg = <1>;
			qcom,supply-name = "vdd";
			qcom,supply-min-voltage = <3000000>;
			qcom,supply-max-voltage = <3000000>;
			qcom,supply-enable-load = <857000>;
			qcom,supply-disable-load = <0>;
			qcom,supply-post-on-sleep = <0>;
		};
	};
};

&pm8150l_wled {
	status = "ok";
};

&pm8150l_lcdb {
	status = "ok";
};

&sde_dsi {
	qcom,dsi-default-panel = <&dsi_ili9806e_fwvga_video>;
	lab-supply = <&lcdb_ldo_vreg>;
	ibb-supply = <&lcdb_ncp_vreg>;
};

&pm8150l_pwm {
	status = "ok";
	pinctrl-names = "default";
	pinctrl-0 = <&pwm_out_gpio06_default>;
};

&pm8150l_gpios {
	status = "ok";
	pwm_out_gpio06 {
		pwm_out_gpio06_default: pwm_out_gpio06_default {
			pins = "gpio6";
			function = "func1";
			bias-disable;
			power-source = <0>;
			output-low;
			qcom,drive-strength = <3>;
			drive-push-pull;
		};
	};
	pwr_src_det_gpio04 {
		gpio_power_src_det: gpio_power_src_det {
			pins = "gpio4";
			function = "normal";
			bias-pull-up;
		};
	};
};

&dsi_ili9806e_fwvga_video {
	qcom,panel-supply-entries = <&dsi_openq_panel_pwr_supply>;
	qcom,mdss-dsi-bl-pmic-control-type = "bl_ctrl_pwm";
	pwms = <&pm8150l_pwm 0 0>;
	qcom,bl-pmic-pwm-period-usecs = <100>;
	qcom,platform-te-gpio = <&tlmm 66 0>;
	qcom,platform-reset-gpio = <&tlmm 75 0>;
	qcom,dsi-select-clocks = "mux_byte_clk0", "mux_pixel_clk0";
	qcom,mdss-dsi-display-timings {
		timing@0 {
			qcom,mdss-dsi-panel-phy-timings =  [00 13 04 04 13 1F
				04 05 04 02 04 00 10 09];
			qcom,display-topology = <1 0 1>;
			qcom,default-topology-index = <0>;
		};
	};
};

&dsi_sim_vid {
	qcom,panel-supply-entries = <&dsi_openq_panel_pwr_supply>;
	qcom,platform-reset-gpio = <&tlmm 75 0>;
	qcom,mdss-dsi-display-timings {
		timing@0 {
			qcom,mdss-dsi-panel-width = <1920>;
			qcom,mdss-dsi-panel-height = <1080>;
		};
	};
};

&dsi_dual_nt36850_truly_cmd {
	qcom,panel-supply-entries = <&dsi_panel_pwr_supply>;
	qcom,mdss-dsi-bl-pmic-control-type = "bl_ctrl_wled";
	qcom,platform-te-gpio = <&tlmm 66 0>;
	qcom,platform-reset-gpio = <&tlmm 75 0>;
	qcom,ulps-enabled;
	qcom,dsi-select-clocks = "mux_byte_clk0", "mux_pixel_clk0";
	qcom,mdss-dsi-display-timings {
		timing@0 {
			qcom,mdss-dsi-panel-phy-timings =  [00 1f 08 08 24 23 08
				08 05 03 04 00 30 0e];
			qcom,display-topology = <2 0 2>, <1 0 2>;
			qcom,default-topology-index = <0>;
		};
	};
};

/*General 1.8v rail*/
&pm8150_s4 {
	regulator-max-microvolt = <1800000>;
};

/*Display Vdd*/
&pm8150a_l11 {
	regulator-min-microvolt = <3000000>;
	qcom,init-voltage = <3000000>;
};

/* SMB1390 is absent from Open-Q 865 SOM */
&smb1390 {
	status = "disabled";
};

&smb1390_charger {
	status = "disabled";
};

&smb1390_slave {
	status = "disabled";
};

&smb1390_slave_charger {
	status = "disabled";
};

&pm8150b_charger {
	qcom,sec-charger-config = <0>; /* no secondary charger */
	qcom,battery-data = <&openq_batterydata>;
	/delete-property/ qcom,fcc-stepping-enable;
	qcom,usb-icl-ua = <1500000>;
};

&pm8150b_pdphy {
	qcom,default-sink-caps = <5000 1500>, /* 5V @ 1.5A */
				 <9000 1500>, /* 9V @ 1.5A */
				 <12000 1500>; /* 12V @ 1.5A */
};

&pm8150b_fg {
	qcom,battery-data = <&openq_batterydata>;
	/delete-property/ qcom,five-pin-battery;
	qcom,pwr-src-det-gpio = <&pm8150l_gpios 4 0>;
	pinctrl-names = "default";
	pinctrl-0 = <&gpio_power_src_det>;
};

